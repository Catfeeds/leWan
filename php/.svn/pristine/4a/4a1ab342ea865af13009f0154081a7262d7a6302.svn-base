					var time = parseInt(endtime) - new Date().getTime();
					if(time <= 0) {
						return '结束'
					} else {
						dd = Math.floor(time / 60 / 60 / 24);
						hh = Math.floor((time / 60 / 60) % 24);
						mm = Math.floor((time / 60) % 60);
						ss = Math.floor(time % 60);
