<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title>个人中心</title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="../../css/mui.min.css" rel="stylesheet" />
		<link href="../../css/leWan.css" rel="stylesheet" />
		<link href="../../plugins/swiper-4.3.3.min.css" rel="stylesheet" />
		<link href="../../fonts/iconfont.css" rel="stylesheet" />
		<style type="text/css">
			body {
				padding: 0;
				margin: 0;
			}
			/*底部选项卡*/
			.mui-bar-tab i{
				font-size: 24px;
			}
			.mui-bar-tab .mui-tab-item{
				color: #CCCCCC;
			}
			.mui-bar-tab .mui-tab-item.mui-active{
				color: #333333;
				font-weight: bold;
			}
			.mui-bar{
				box-shadow: 0px -1px 10px 1px #e7e7e7;
			}
			
			/*页头*/
			.headerBox{
				height: 4.64rem;
				overflow: hidden;
				text-align: center;
				background-size: 100%;
				background-repeat: no-repeat;
			}
			.powerExplain{
				font-size: 0.346rem;
				color: #FFFFFF;
				text-align: right;
				margin-right: 0.32rem;
				margin-top: 0.1rem;
			}
			.headPortrait{
				width: 1.866rem;
				height: 1.866rem;
				border-radius: 0.933rem;
				background-size: 100%;
				background-repeat: no-repeat;
				display: inline-block;
			}
			.runLevel{
				font-size: 0.373rem;
				color: #FFFFFF;
			}
			.runLevelImg{
				display: inline-block;
				width: 0.533rem;
				height: 0.533rem;
				border-radius: 0.266rem;
				background-color: #F76E1E;
			}
			.headerCardBox{
				position: relative;
				height: 1.666rem;
				background-color: #FFFFFF;
			}
			.headerCard{
				width: 7.173rem;
				height: 2.666rem;
				display: flex;
				justify-content: space-around;
				align-items: center;
				border: 1px solid #F2F2F2;
				background-color: #FFFFFF;
				border-radius: 0.2rem;
				box-shadow: #F6D9A3 0px 0px 10px;
				position: absolute;
				top: -1rem;
				left: 0;
				right: 0;
				margin: 0 auto;
			}
			.headerCard>div:nth-child(2){
				padding: 0 0.4rem;
				border-right: 1px solid #e7e7e7;
				border-left: 1px solid #e7e7e7;
			}
			.headerCard p{
				font-size: 0.346rem;
				color: #999999;
				font-weight: 500;
			}
			.headerCard>div>div{
				font-size: 0.533rem;
				color: #333333;
				font-weight: bold;
				margin-top: 0.266rem;
			}
			.mui-badge{
				background: #FFFFFF;
				padding: 0;
				position: relative;
				right: 0;
				top: -0.25rem;
				font-weight: bold;
			}
			.addUser{
				color: #FBD086;
			}
			.removeUesr{
				color: #DDD4D4;
			}
			/*我的钱包*/
			.cardBox{
				text-align: center;
			}
			.content{
				background-color: #FFFFFF;
			}
			.title{
				font-size: 0.506rem;
				color: #333333;
				font-weight: bold;
				padding-top: 0.533rem;
				margin-left: 0.5rem;
			}
			.cardBox1{
				display: inline-block;
				margin-top: 0.533rem;
				width: 9.04rem;
				height: 4.16rem;
				border-radius: 0.2rem;
				background: -webkit-gradient(linear, left right, color-stop(100%,#BCBCBC));
				background: linear-gradient(to right, #171717 0%,#BCBCBC 100%);
			}
			.cardBoxOne{
				overflow: hidden;
				padding: 0.426rem 0.453rem 0 0.453rem;
			}
			.cardBoxOne>span:nth-child(1){
				font-size: 0.373rem;
				color: #666666;
				font-weight: 500;
				float: left;
			}
			.cardBoxOne>span:nth-child(2){
				font-size: 0.346rem;
				color: #FFFFFF;
				font-weight: 500;
				float: right;
			}
			.cardBoxTwo{
				position: relative;
				margin-top: 0.266rem;
			}
			.cardBoxTwo>span:nth-child(1){
				font-size: 0.906rem;
				color: #E1B872;
				font-weight: normal;
				float: left;
				margin-left: 0.453rem;
				display: inline-block;
				height: 1.12rem;
				line-height: 1.12rem;
			}
			.cardBoxTwo>span:nth-child(2){
				font-size: 0.533rem;
				color: #FFFFFF;
				font-weight: bold;
				display: inline-block;
				width: 2.853rem;
				height: 1.12rem;
				line-height: 1.12rem;
				background-color: #D7D7D7;
				box-shadow: #666666 0px 0px 10px;
				border-top-left-radius: 0.56rem;
				border-bottom-left-radius: 0.56rem;
				position: absolute;
				top: 0;
				right: -0.213rem;
			}
			.cardBoxThree{
				clear: both;
				padding-top: 0.266rem;
				display: flex;
				justify-content: space-around;
				align-items: center;
			}
			.cardBoxThree p{
				font-size: 0.32rem;
				color: #CCCCCC;
				font-weight: 500;
			}
			.cardBoxThree>div>div{
				font-size: 0.346rem;
				color: #FFFFFF;
				font-weight: 500;
				margin-top: 0.1rem;
			}
			/*统计卡片*/
			.cardBox2{
				text-align: center;
				margin-top: 0.533rem;
			}
			.cardBox2>div{
				display: inline-block;
			}
			.cardBoxYi{
				width: 9.04rem;
				height: 3.413rem;
				background-size: 100%;
				background-repeat: no-repeat;
				border-radius: 0.2rem;
				display: flex;
				justify-content: space-around;
				align-items: center;
				margin-bottom: 0.533rem;
			}
			
			.cardBoxYi p{
				color: #E1B872;
				font-size: 0.426rem;
				font-weight: bold;
			}
			.cardBoxYi>div>div{
				color: #999999;
				font-size: 0.426rem;
				font-weight: bold;
				margin-top: 0.266rem;
			}
			/*导航列表*/
			.menuList{
				margin-top: 0.266rem;
			}
			.mui-table-view:before{
				height: 0;
			}
			.menuList a{
				font-size: 0.426rem;
				color: #333333;
				font-weight: bold;
			}
			.menuList i{
				font-size: 0.6rem;
				color: #E1B872;
				font-weight: bold;
				margin-right: 0.293rem;
			}
			.mui-navigate-right:after, .mui-push-right:after{
				font-size: 0.6rem;
			}
		</style>
	</head>

	<body>
		<!--底部导航-->
		<nav class="mui-bar mui-bar-tab">
			<a class="mui-tab-item" id="homePage">
				<span class="mui-icon"><i class="iconfont icon-shouye"></i></span>
				<span class="mui-tab-label">首页</span>
			</a>
			<a class="mui-tab-item" id="everydayFaddish">
				<span class="mui-icon"><i class="iconfont icon-huo"></i></span>
				<span class="mui-tab-label">每日爆款</span>
			</a>
			<a class="mui-tab-item" id="reservationCenter">
				<span class="mui-icon"><i class="iconfont icon-chakantiezishijian"></i></span>
				<span class="mui-tab-label">预约中心</span>
			</a>
			<a class="mui-tab-item mui-active">
				<span class="mui-icon"><i class="iconfont icon-wode"></i></span>
				<span class="mui-tab-label">我的</span>
			</a>
		</nav>
		<div class="header">
			<div class="headerBox" style="background-image: url(../../img/personalCenter/perCenter1.jpg);">
				<div class="powerExplain" id="equityExplain">权益说明？</div>
				<div class="headPortrait" style="background-image: url(../../img/homePage/eightNav1.png);"></div>
				<div class="runLevel">
					<span class="runLevelImg"><i class="iconfont icon-yousanjiao"></i></span>
					<span>运营达人</span>
				</div>
			</div>
			<div class="headerCardBox">
				<div class="headerCard">
					<div class="" id="myClient">
						<p>我的客户</p>
						<div>189<i class="mui-badge addUser">+5</i></div>
					</div>
					<div class="myFriend" id="relativeFriend" dataNum="0">
						<p>直属好友</p>
						<div>189<i class="mui-badge addUser">+3</i></div>
					</div>
					<div class="myFriend" id="allFriend" dataNum="1">
						<p>全部好友</p>
						<div>189<i class="mui-badge removeUesr">-5</i></div>
					</div>
				</div>
			</div>
		</div>
		<div class="content">
			<div class="title">我的钱包</div>
			<div class="cardBox">
				<div class="cardBox1">
					<div class="cardBoxOne">
						<span>可提现佣金</span>
						<span id="incomeDetail">收入明细 <i class="iconfont icon-yousanjiao"></i></span>
					</div>
					<div class="cardBoxTwo">
						<span>835.00</span>
						<span id="toWithdraw">去提现</span>
					</div>
					<div class="cardBoxThree">
						<div class="">
							<p>待结算</p>
							<div>76.00</div>
						</div>
						<div class="">
							<p>累计结算</p>
							<div>76.00</div>
						</div>
						<div class="">
							<p>累计提现</p>
							<div>716.00</div>
						</div>
					</div>
				</div>
				
				
			</div>
			<div class="cardBox2">
				<div class="">
					<div class="cardBoxYi" style="background-image: url(../../img/personalCenter/perCenter2.jpg);">
						<div class="">
							<p>今 日</p>
							<div>76</div>
						</div>
						<div class="">
							<p>昨 日</p>
							<div>76</div>
						</div>
						<div class="">
							<p>本 月</p>
							<div>716</div>
						</div>
						<div class="">
							<p>上 月</p>
							<div>726</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="menuList">
			<ul class="mui-table-view">
			    <li class="mui-table-view-cell" id="inviteFriends">
			        <a class="mui-navigate-right"><i class="iconfont icon-yaoqingma"></i>邀请好友</a>
			    </li>
			    <li class="mui-table-view-cell">
			        <a class="mui-navigate-right" id="newbieCourse"><i class="iconfont icon-shuoming"></i>新手教程</a>
			    </li>
			    <li class="mui-table-view-cell">
			        <a class="mui-navigate-right" id="coupon"><i class="iconfont icon-youhuiquan"></i>优惠券</a>
			    </li>
			    <li class="mui-table-view-cell">
			        <a class="mui-navigate-right" id="myIndent"><i class="iconfont icon-shouye1"></i>我的订单</a>
			    </li>
			    <li class="mui-table-view-cell">
			        <a class="mui-navigate-right" id="shippingAddress"><i class="iconfont icon-bianji2"></i>收货地址</a>
			    </li>
			    <li class="mui-table-view-cell">
			        <a class="mui-navigate-right" id="contactUs"><i class="iconfont icon-lianxiren"></i>联系我们</a>
			    </li>
			    
			</ul>
		</div>
		<script src="../../plugins/flexible.js"></script>
		<script src="../../plugins/jquery-2.1.4.js"></script>
		<script src="../../plugins/vue.js"></script>
		<script src="../../js/mui.min.js"></script>
		<script src="../../plugins/swiper-4.3.3.min.js"></script>
		<script src="../../plugins/serverApi.js"></script>
		<script type="text/javascript">
			$(function() {
				
				mui('body').on('tap','#equityExplain',function(){	//权益说明
					mui.openWindow({
						url: "equityExplain.html",
						id: "equityExplain.html"
					})
				})
				mui('body').on('tap','#myClient',function(){	//我的客户
					mui.openWindow({
						url: "myClient.html",
						id: "myClient.html"
					})
				})
				mui('body').on('tap','.myFriend',function(){	//直属好友，全部好友
					var dataNum = $(this).attr("dataNum");		//0为直属好友，1为全部好友
					mui.openWindow({
						url: "myFriend.html?dataNum="+dataNum,
						id: "myFriend.html"
					})
				})
				mui('body').on('tap','#incomeDetail',function(){	//收入明细
					mui.openWindow({
						url: "incomeDetail.html",
						id: "incomeDetail.html"
					})
				})
				mui('body').on('tap','#toWithdraw',function(){	//去提现
					mui.openWindow({
						url: "toWithdraw.html",
						id: "toWithdraw.html"
					})
				})
				mui('body').on('tap','#inviteFriends',function(){	//邀请好友
					mui.openWindow({
						url: "inviteFriends.html",
						id: "inviteFriends.html"
					})
				})
				mui('body').on('tap','#newbieCourse',function(){	//新手教程
					mui.openWindow({
						url: "newbieCourse.html",
						id: "newbieCourse.html"
					})
				})
				mui('body').on('tap','#coupon',function(){	//优惠券
					mui.openWindow({
						url: "coupon.html",
						id: "coupon.html"
					})
				})
				mui('body').on('tap','#myIndent',function(){	//我的订单
					mui.openWindow({
						url: "myIndent.html",
						id: "myIndent.html"
					})
				})
				mui('body').on('tap','#shippingAddress',function(){	//收货地址
					mui.openWindow({
						url: "shippingAddress.html",
						id: "shippingAddress.html"
					})
				})
				mui('body').on('tap','#contactUs',function(){	//联系我们
					mui.openWindow({
						url: "contactUs.html",
						id: "contactUs.html"
					})
				})
			})
		</script>
	</body>

</html>