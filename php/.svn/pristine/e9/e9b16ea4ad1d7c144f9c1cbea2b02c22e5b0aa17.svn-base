/*
 *这是单独为一些公用样式库
 * 
 * */
/*对象选择器 {box-shadow:投影方式 X轴偏移量 Y轴偏移量 阴影模糊半径 阴影扩展半径 阴影颜色}*/
/*渐变色设置background: linear-gradient(to right, #171717 0%,#BCBCBC 100%);*/

a {
	text-decoration: none !important;
}
body{
	margin-bottom: 2rem !important;
}

/*空状态*/
.lw-empty {
	display: none
}
.lw-empty.lw-empty-bgf {
	background: #fff;
}
.lw-empty-box {
	width: 100%;
	height: 11rem;
	display: flex;
	display: -webkit-flex;
	align-items: center;
	-webkit-align-items: center;
	justify-content: center;
	-webkit-justify-content: center;
}
.lw-empty-box .lw-empty-flex{
	text-align: center;
}
.lw-empty-box .lw-empty-test{
	color: #999999;
	font-size: 0.346rem;
	margin-top: 0.2rem;
}

.lw-empty-box .lw-empty-img {
	width: 3.733rem;
	height: 3.733rem;
}
