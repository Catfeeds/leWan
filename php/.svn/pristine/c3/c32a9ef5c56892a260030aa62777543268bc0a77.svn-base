									token = data.data.token;
									localStorage.setItem("level", data.data.level);
									vueData.level = data.data.level;
								} else {
									mui.toast(data.message)
								}
							}
						})
					}
				}
				//锁粉
				suofen()
				function suofen(){
					$.ajax({
						url: API_SERVER + 'User/UserLockPowder',
						async: false,
						data:{
							token: token,
							recode: localStorage.getItem("homePageRecode") || null
						},
						success: function(data) {
							log(data)
						}
					});
				}
				
				//获取地理位置
